<div class="hero-unit">
  <h2 class="id"><%= {name:Paul}.id %></h2>
  <%- linkTo('Edit this {name:Paul}', edit{name:Paul}Path(params.id), {class: 'btn pull-right'}); %>
</div>

<h2>{name:Paul} Properties</h2>
<% for(var i in {name:Paul}) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= {name:Paul}[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>